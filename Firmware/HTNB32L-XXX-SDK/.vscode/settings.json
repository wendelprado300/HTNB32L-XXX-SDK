{
    "makefile.extensionOutputFolder": "./.vscode",
    "cortex-debug.variableUseNaturalFormat": true,
    "cortex-debug.gdbPath": "C:/gcc_arm/bin/arm-none-eabi-gdb",
    "C_Cpp.errorSquiggles": "enabled",
    "files.associations": {
        "*.inc": "bitbake",
        "*.rmd": "markdown",
        "cmsis_gcc.h": "c",
        "stdio.h": "c",
        "qcx212.h": "c",
        "htnb32lxxx_hal_spi.h": "c",
        "ht_spi_demo.h": "c",
        "stdint.h": "c",
        "ht_gpio_api.h": "c",
        "ht_mqtt_api.h": "c",
        "uart_qcx212.h": "c",
        "random": "c",
        "driver_common.h": "c",
        "ht_peripheral_config.h": "c",
        "driver_usart.h": "c",
        "array": "c",
        "string": "c",
        "string_view": "c",
        "ranges": "c",
        "ht_hal_uart.h": "c",
        "main.h": "c",
        "htnb32lxxx_hal_usart.h": "c",
        "ht_usart_demo.h": "c",
        "initializer_list": "c",
        "utility": "c",
        "ht_plat_config.h": "c",
        "ht_uart_unilog.h": "c",
        "string.h": "c",
        "ps_lib_api.h": "c",
        "ht_bsp.h": "c",
        "ht_uart_api.h": "c",
        "ht_timer_demo.h": "c",
        "ht_usart_unilog.h": "c",
        "*.conf": "bitbake"
    },
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/sources/poky/bitbake/lib",
        "${workspaceFolder}\\sources\\poky\\meta\\lib"
    ],
    "python.analysis.extraPaths": [
        "${workspaceFolder}/sources/poky/bitbake/lib",
        "${workspaceFolder}\\sources\\poky\\meta\\lib"
    ],
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "off",
        "files.trimTrailingWhitespace": false
    },
    "[shellscript]": {
        "files.eol": "\n",
        "files.trimTrailingWhitespace": false
    }
}
